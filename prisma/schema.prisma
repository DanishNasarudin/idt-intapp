generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ap_local {
  service_no     String  @id @db.VarChar(255)
  date           String? @db.VarChar(255)
  pic            String? @db.VarChar(20)
  name           String? @db.VarChar(100)
  contact        String? @db.VarChar(40)
  status         String? @db.VarChar(255)
  email          String? @db.VarChar(255)
  address        String? @db.Text
  purchase_date  String? @db.VarChar(255)
  invoice        String? @db.VarChar(255)
  received_items String? @db.VarChar(250)
  pin            String? @db.Text
  issues         String? @db.Text
  solutions      String? @db.Text
  status_desc    String? @db.Text
  remarks        String? @db.Text
  cost           Int?    @default(0)
  locker         Int?
  received_by    String? @db.VarChar(20)
  idt_pc         String? @db.VarChar(10)

  @@index([contact], map: "contact_idx")
  @@index([email], map: "email_idx")
  @@index([name], map: "name_idx")
  @@index([received_items], map: "received_items_idx")
}

model ap_local_history {
  history_id       Int                          @id @default(autoincrement())
  service_no       String?                      @db.VarChar(255)
  date             String?                      @db.VarChar(255)
  pic              String?                      @db.VarChar(20)
  name             String?                      @db.Text
  contact          String?                      @db.Text
  status           String?                      @db.VarChar(255)
  email            String?                      @db.VarChar(255)
  address          String?                      @db.Text
  purchase_date    String?                      @db.VarChar(255)
  invoice          String?                      @db.VarChar(255)
  received_items   String?                      @db.VarChar(250)
  pin              String?                      @db.Text
  issues           String?                      @db.Text
  solutions        String?                      @db.Text
  status_desc      String?                      @db.Text
  remarks          String?                      @db.Text
  cost             Int?                         @default(0)
  locker           Int?
  received_by      String?                      @db.VarChar(20)
  idt_pc           String?                      @db.VarChar(10)
  change_type      ap_local_history_change_type
  change_timestamp DateTime?                    @default(now()) @db.DateTime(0)
}

